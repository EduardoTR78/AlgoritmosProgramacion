Algoritmo CON03
	Definir Numero como entero;
	Escribir "Introduzca Numero entero:";
	Leer Numero;
	Si Numero %5 = 0 entonces
		Escribir "El numero introducido es multiplo a 5:";
	SiNo
		Escribir "El numero introducido no es multiplo a 5:";
	FinSi
FinAlgoritmo

