//00597299 EDUARDO TELLEZ RAMIREZ

Algoritmo CON10
	Definir Edad Como entero;
	Definir Genero como Caracter;
	Escribir "Introduzca su edad:";
	Leer Edad;
	Escribir "Introduzca Genero:";
	Leer Genero;
	Si Edad > 18 entonces;
		Escribir "Puede Ingresar:";
	Sino 
		Escribir "No puede Ingresar:";
		
		Si Genero = Masculino Entonces;
			Escribir "Puede ingresar";
		Sino 
			Escribir "No puede ingresar";
		FinSi
	FinSi
FinAlgoritmo

