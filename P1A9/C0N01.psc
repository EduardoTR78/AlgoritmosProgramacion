Algoritmo C0N01
	Definir Numero como entero;
	Escribir "Introduzca numero entero:";
	Leer Numero;
	Si Numero > 0 entonces 
		Escribir "El numero es positivo:";
	Sino Si Numero = 0 entonces
			Escribir "El cero no tiene signo:";
		
		     Si Numero < 0 entonces 
		      
             Escribir "El numero es negativo"
		     
		     FinSi
		 
		FinSi
	FinSi
FinAlgoritmo
