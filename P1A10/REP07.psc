Algoritmo REP07
	Definir N, Suma como Reales;
	Definir I como entero;
	I <- 0 
	Suma <- 0 
	Repetir
		Escribir "Introduzca valor de N:" 
		Leer N;
		Suma <- Suma + N;
		I <- I + 1
	Hasta Que I = 5
	Escribir "La suma de los 5 numeros es:", Suma;
	Escribir "La media de los numeros es:", Suma / 5 
	
FinAlgoritmo
